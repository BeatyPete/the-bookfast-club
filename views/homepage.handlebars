<div class="book-bg flex justify-around items-center">

    <button id="prev" class="page-btn">prev</button>

    <div class="post-grid grid grid-cols-2 gap-2">
        {{#each posts as |post|}}
            {{> post-info post}}
        {{/each}}
    </div>

    <button id="next" class="page-btn">next</button>

</div>

<div class="page-num">
    <p id="left-pg"></p>
    <p id="right-pg"></p>
</div>

